#include <iostream>
#include <cstdlib>

using namespace std;

int RandomNumber(int From, int To)
{
	int randNum = rand() % (To - From + 1) + From;
	return randNum;
}

void FillRandomMatrix(int arr2d[3][3])
{
	for (int i = 0; i < 3; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			arr2d[i][j] = RandomNumber(1, 100);
		}
	}
}

void PrintRandomMatrix(int arr2d[3][3])
{
	cout << "The following is a 3x3 random matrix: " << endl;
	for (int i = 0; i < 3; i++)
	{
		for (int j = 0; j < 3; j++)
		{
			cout << arr2d[i][j] << "\t";
		}
		cout << endl;
	}
}
int main()
{
	srand(static_cast<unsigned int>(time(0)));
	int arr2d[3][3];

	FillRandomMatrix(arr2d);
	PrintRandomMatrix(arr2d);

}
