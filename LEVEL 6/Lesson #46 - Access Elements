#include <iostream>
#include <vector>

using namespace std;


int main()
{
	vector <int> v1{ 1,2,3,4,5 };

	// you can access elements of vector using 2 methods:
	// 1. .at()

	cout << "Index 0 elements " << v1.at(0) << endl;
	cout << "Index 1 elements " << v1.at(1) << endl;

	cout << "=====================================" << endl;
	// 2. []
	cout << "Index 0 elements " << v1[0] << endl;
	cout << "Index 1 elements " << v1[1] << endl;
}
